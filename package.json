{
  "name": "nasa-telegram-bot",
  "version": "1.0.0",
  "description": "Telegram bot for NASA API",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development dotenv -e .env -- nodemon --exec tsx src/main.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production dotenv -e .env -- node ./dist/main.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": ["nasa", "telegram", "bot", "space"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "telegraf": "^4.12.2"
  }
}
